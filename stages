pipeline {
    agent any

    stages {
        stage('Development') {
            steps {
                echo 'Hello Dev'
            }
        }
        
        stage('testing') {
            steps {
                echo 'Hello Testing'
            }
        }
        
        stage('pre-prod') {
            steps {
                echo 'Hello Pre-prod'
            }
        }
        
        stage('prod') {
            steps {
                echo 'Hello prod'
            }
        }
    }
}
